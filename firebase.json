{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/no/**",
        "destination": "/index.html"
      },
      {
        "source": "/es/**",
        "destination": "/index.html"
      },
      {
        "source": "/dk/**",
        "destination": "/index.html"
      },
      {
        "source": "/se/**",
        "destination": "/index.html"
      },
      {
        "source": "/fi/**",
        "destination": "/index.html"
      },
      {
        "source": "/nl/**",
        "destination": "/index.html"
      },
      {
        "source": "/de/**",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}